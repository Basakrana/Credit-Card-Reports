      'public cust_details'[customer_age] >=40 && 'public cust_details'[customer_age] < 50, "40-50",
      'public cust_details'[customer_age] >=50 && 'public cust_details'[customer_age] < 60, "50-60",
      'public cust_details'[customer_age] >= 60, "60+",
      "unknownâ€œ
)

